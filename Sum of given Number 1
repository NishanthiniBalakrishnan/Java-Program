import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
        int sum=0,num;
        do{
           num=sc.nextInt();
             sum+=num;
          
        }while(num!=-1);
        
         System.out.print(sum+1);
         
            
        
  
    
    }
}
